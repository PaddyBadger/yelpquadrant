  <body>
  <header>
    <h1>Quadrants</h1>
    <form action="/send" method="get" id="form">
      <label>Search: </label><input type="text" name="category" id="search"<br>
      <label>Location: </label> <input type="text" name="location" id="location">
      <input class="submit" type="submit" value="Search">
